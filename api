message IdentifyUserID {
    string ID;
}

message IdentifyUsername {
    option from = "client";
    string username;
}

message RoomList {
    repeated string roomList;
}

message CreateRoom {
    option from = "client";
    string pathname;
}

message JoinRoom {
    option from = "both";
    string roomID;
    string hostID;
}

message UserJoined {
    option from = "server";
    string username;
    string userID;
}

message Message {
    option from = "server";
    string ID;
    string username;
    string message; 
    string userID;
}

message Message {
    option from = "client";
    string message;
}

message GetRoomList {
    option from = "client";
}

message Timeupdate {
    option from = "both";
    int64 time;
}
message Play {
    option from = "both";
}
message Pause {
    option from = "both";
}